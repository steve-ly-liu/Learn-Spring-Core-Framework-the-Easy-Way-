<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="sampleBean" class="org.company.defaultpropertyeditor.SampleBean">
        <constructor-arg type="java.lang.Integer" value="12345"/>
        <constructor-arg type="java.lang.Double" value="23456.123"/>
        <constructor-arg type="java.lang.Character" value="@" />
        <constructor-arg type="java.lang.String" value="string-value" />
        <constructor-arg type="java.util.Currency" value="CAD" />
        <constructor-arg type="java.net.URL" value="http://www.redhat.com" />
    </bean>

</beans>